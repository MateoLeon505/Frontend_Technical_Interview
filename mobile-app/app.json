{
  "expo": {
    "name": "mobile-app",
    "slug": "mobile-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "dark",
    "splash": {
      "image": "./src/assets/icon.png",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mateoleon505.mobileapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/icon.png"
      },
      "package": "com.mateoleon505.mobileapp"
    },
    "scheme": "mobile-app",
    "web": {
      "favicon": "./src/assets/icon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d0672402-0e88-4726-be8d-4083ca7c1a03"
      }
    }
  }
}
